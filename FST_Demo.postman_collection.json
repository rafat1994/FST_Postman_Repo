	"item": [
		{
			"name": "RESTAPI_GET_Request",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"pm.test(\"Body matches string\", function () {\r",
							"    pm.expect(pm.response.text()).to.include(\"Michael\");\r",
							"});\r",
							"\r",
							"pm.test(\"Your test name\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.data[0].last_name).to.eql(\"Lawson\");\r",
							"});\r",
							"pm.test(\"Response time is less than 2000ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(2000);\r",
							"});\r",
							"\r",
							"pm.test(\"header validation\", ()=>{\r",
							"    pm.response.to.have.header(\"Content-Type\");\r",
							"    var headervalidation=pm.response.headers.get(\"Content-Type\");\r",
							"    pm.expect(headervalidation).to.include(\"json\");\r",
							"    console.log(headervalidation);\r",
							"})"
						],
						"type": "text/javascript"
					}
				}
			],
